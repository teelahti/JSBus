<?xml version="1.0"?>
<package >
  <metadata>
    <id>JSBus</id>
    <version>1.2.1</version>
    <authors>Tero Teelahti</authors>
    <owners>Tero Teelahti</owners>
    <licenseUrl>https://raw.github.com/teelahti/JSBus/master/LICENSE.md</licenseUrl>
